[
  { "id": 1, "name": "악취", "text": "악취 때문에 상대가\n풀죽을 때가 있다." },
  { "id": 2, "name": "잔비", "text": "배틀에 나가면\n비를 내린다." },
  { "id": 3, "name": "가속", "text": "조금씩\n스피드가 높아진다." },
  { "id": 4, "name": "전투무장", "text": "상대의 공격이\n급소에 맞지 않는다." },
  { "id": 5, "name": "옹골참", "text": "일격으로\n쓰러지지 않는다." },
  { "id": 6, "name": "습기", "text": "누구도 폭발\n할 수 없게 된다." },
  { "id": 7, "name": "유연", "text": "마비 상태가\n되지 않는다." },
  { "id": 8, "name": "모래숨기", "text": "모래바람으로\n회피율이 올라간다." },
  { "id": 9, "name": "정전기", "text": "접촉한 상대를\n마비시킬 때가 있다." },
  { "id": 10, "name": "축전", "text": "전기를 받으면\n회복한다." },
  { "id": 11, "name": "저수", "text": "물을 받으면\n회복한다." },
  {
    "id": 12,
    "name": "둔감",
    "text": "헤롱헤롱이나 도발 상태가\n되지 않는다."
  },
  { "id": 13, "name": "날씨부정", "text": "날씨의 영향이\n없어진다." },
  { "id": 14, "name": "복안", "text": "기술의 명중률이\n올라간다." },
  { "id": 15, "name": "불면", "text": "잠듦 상태가\n되지 않는다." },
  { "id": 16, "name": "변색", "text": "받은 기술의 타입으로\n변화한다." },
  { "id": 17, "name": "면역", "text": "독 상태가\n되지 않는다." },
  {
    "id": 18,
    "name": "타오르는불꽃",
    "text": "불꽃을 받으면\n불꽃 기술이 강해진다."
  },
  { "id": 19, "name": "인분", "text": "기술의 추가 효과를\n받지 않는다." },
  { "id": 20, "name": "마이페이스", "text": "혼란 상태가\n되지 않는다." },
  {
    "id": 21,
    "name": "흡반",
    "text": "교체시키는 기술이나 도구의\n효과를 받지 않는다."
  },
  { "id": 22, "name": "위협", "text": "상대의 공격을\n떨어뜨린다." },
  {
    "id": 23,
    "name": "그림자밟기",
    "text": "상대의 그림자를 밟아\n도망칠 수 없게 한다."
  },
  { "id": 24, "name": "까칠한피부", "text": "접촉한 상대에게\n상처를 입힌다." },
  {
    "id": 25,
    "name": "불가사의부적",
    "text": "효과가 굉장한 기술밖에\n맞지 않는다."
  },
  { "id": 26, "name": "부유", "text": "땅타입의 기술을\n받지 않는다." },
  {
    "id": 27,
    "name": "포자",
    "text": "스치면 독, 마비, 잠듦\n상태가 될 때가 있다."
  },
  { "id": 28, "name": "싱크로", "text": "독, 마비, 화상을\n상대에게 옮긴다." },
  {
    "id": 29,
    "name": "클리어바디",
    "text": "상대가 능력을\n떨어뜨릴 수 없다."
  },
  {
    "id": 30,
    "name": "자연회복",
    "text": "배틀에서 일단 물러나면\n상태 이상이 회복된다."
  },
  { "id": 31, "name": "피뢰침", "text": "전기를 끌어모아\n특수공격을 올린다." },
  {
    "id": 32,
    "name": "하늘의은총",
    "text": "기술의 추가 효과가\n나오기 쉽다."
  },
  { "id": 33, "name": "쓱쓱", "text": "비가 올 때 스피드가\n올라간다." },
  { "id": 34, "name": "엽록소", "text": "맑을 때 스피드가\n올라간다." },
  { "id": 35, "name": "발광", "text": "야생 포켓몬과\n만나기 쉬워진다." },
  { "id": 36, "name": "트레이스", "text": "상대와 같은\n특성이 된다." },
  { "id": 37, "name": "천하장사", "text": "물리공격의\n위력이 올라간다." },
  { "id": 38, "name": "독가시", "text": "접촉한 상대를\n중독시킬 때가 있다." },
  { "id": 39, "name": "정신력", "text": "풀죽지 않는다." },
  { "id": 40, "name": "마그마의무장", "text": "얼음 상태가\n되지 않는다." },
  { "id": 41, "name": "수의베일", "text": "화상 상태가\n되지 않는다." },
  { "id": 42, "name": "자력", "text": "강철의 포켓몬을\n도망칠 수 없게 한다." },
  { "id": 43, "name": "방음", "text": "소리 기술을\n받지 않는다." },
  { "id": 44, "name": "젖은접시", "text": "비가 올 때 조금씩\nHP를 회복한다." },
  { "id": 45, "name": "모래날림", "text": "배틀에서 모래바람을\n일으킨다." },
  {
    "id": 46,
    "name": "프레셔",
    "text": "상대가 사용하는 기술의\nPP를 많이 줄인다."
  },
  {
    "id": 47,
    "name": "두꺼운지방",
    "text": "불꽃과 얼음타입의\n기술에 강하다."
  },
  { "id": 48, "name": "일찍기상", "text": "잠듦 상태에서\n빨리 깨어난다." },
  {
    "id": 49,
    "name": "불꽃몸",
    "text": "접촉한 상대에게\n화상을 입힐 때가 있다."
  },
  {
    "id": 50,
    "name": "도주",
    "text": "야생 포켓몬으로부터\n반드시 도망칠 수 있다."
  },
  { "id": 51, "name": "날카로운눈", "text": "명중률이\n떨어지지 않는다." },
  { "id": 52, "name": "괴력집게", "text": "상대가 공격을\n떨어뜨리지 못한다." },
  { "id": 53, "name": "픽업", "text": "도구를 주워올\n때가 있다." },
  { "id": 54, "name": "게으름", "text": "연속으로\n공격할 수 없다." },
  { "id": 55, "name": "의욕", "text": "공격은 높지만\n빗나가기 쉽다." },
  {
    "id": 56,
    "name": "헤롱헤롱바디",
    "text": "스치면 헤롱헤롱 상태가\n될 때가 있다."
  },
  {
    "id": 57,
    "name": "플러스",
    "text": "플러스나 마이너스가 있으면\n특수공격이 올라간다."
  },
  {
    "id": 58,
    "name": "마이너스",
    "text": "플러스나 마이너스가 있으면\n특수공격이 올라간다."
  },
  { "id": 59, "name": "기분파", "text": "날씨에 따라\n캐스퐁이 변화한다." },
  { "id": 60, "name": "점착", "text": "달라붙어서\n도구를 지킨다." },
  { "id": 61, "name": "탈피", "text": "상태 이상을\n회복할 때가 있다." },
  { "id": 62, "name": "근성", "text": "상태 이상이 되면\n공격이 올라간다." },
  {
    "id": 63,
    "name": "이상한비늘",
    "text": "상태 이상이 되면\n방어가 올라간다."
  },
  { "id": 64, "name": "해감액", "text": "흡수한 상대의\nHP를 줄인다." },
  { "id": 65, "name": "심록", "text": "위급할 때 풀타입의\n위력이 올라간다." },
  {
    "id": 66,
    "name": "맹화",
    "text": "위급할 때 불꽃타입의\n위력이 올라간다."
  },
  { "id": 67, "name": "급류", "text": "위급할 때 물타입의\n위력이 올라간다." },
  {
    "id": 68,
    "name": "벌레의알림",
    "text": "위급할 때 벌레타입의\n위력이 올라간다."
  },
  { "id": 69, "name": "돌머리", "text": "부딪쳐도\n반동을 받지 않는다." },
  { "id": 70, "name": "가뭄", "text": "배틀에 나가면\n햇살이 강해진다." },
  {
    "id": 71,
    "name": "개미지옥",
    "text": "배틀에서 상대를\n도망칠 수 없게 한다."
  },
  { "id": 72, "name": "의기양양", "text": "잠듦 상태가\n되지 않는다." },
  { "id": 73, "name": "하얀연기", "text": "상대가 능력을\n떨어뜨릴 수 없다." },
  { "id": 74, "name": "순수한힘", "text": "물리공격의\n위력이 올라간다." },
  {
    "id": 75,
    "name": "조가비갑옷",
    "text": "상대의 공격이\n급소에 맞지 않는다."
  },
  { "id": 76, "name": "에어록", "text": "날씨의 영향이\n없어진다." },
  {
    "id": 77,
    "name": "갈지자걸음",
    "text": "혼란에 빠져있으면\n회피하기 쉬워진다."
  },
  { "id": 78, "name": "전기엔진", "text": "전기를 받으면\n스피드가 올라간다." },
  { "id": 79, "name": "투쟁심", "text": "상대와 성별이\n같으면 강해진다." },
  {
    "id": 80,
    "name": "불굴의마음",
    "text": "풀죽을 때마다\n스피드가 올라간다."
  },
  {
    "id": 81,
    "name": "눈숨기",
    "text": "날씨가 싸라기눈일 때\n회피율이 올라간다."
  },
  {
    "id": 82,
    "name": "먹보",
    "text": "나무열매를 여느 때보다\n빨리 사용한다."
  },
  { "id": 83, "name": "분노의경혈", "text": "급소에 맞으면\n공격이 올라간다." },
  { "id": 84, "name": "곡예", "text": "도구가 없어지면\n스피드가 올라간다." },
  { "id": 85, "name": "내열", "text": "불꽃 기술의\n위력을 약하게 한다." },
  { "id": 86, "name": "단순", "text": "능력 변화가\n여느 때보다 심하다." },
  {
    "id": 87,
    "name": "건조피부",
    "text": "더우면 HP가 줄어든다.\n물로 HP를 회복한다."
  },
  {
    "id": 88,
    "name": "다운로드",
    "text": "상대의 능력을 보고\n능력치를 바꾼다."
  },
  {
    "id": 89,
    "name": "철주먹",
    "text": "펀치를 사용하는 기술의\n위력이 올라간다."
  },
  { "id": 90, "name": "포이즌힐", "text": "독 상태가 되면\nHP를 회복한다." },
  {
    "id": 91,
    "name": "적응력",
    "text": "타입이 같은 기술의\n위력이 올라간다."
  },
  { "id": 92, "name": "스킬링크", "text": "연속 기술을\n많이 쓸 수 있다." },
  { "id": 93, "name": "촉촉바디", "text": "비가 오면\n상태 이상이 회복된다." },
  {
    "id": 94,
    "name": "선파워",
    "text": "맑으면 HP가 줄지만\n특수공격이 올라간다."
  },
  { "id": 95, "name": "속보", "text": "상태 이상이 되면\n스피드가 올라간다." },
  { "id": 96, "name": "노말스킨", "text": "쓴 기술이 모두\n노말타입이 된다." },
  {
    "id": 97,
    "name": "스나이퍼",
    "text": "급소에 맞혔을 때\n위력이 올라간다."
  },
  {
    "id": 98,
    "name": "매직가드",
    "text": "공격 이외에는\n데미지를 입지 않는다."
  },
  { "id": 99, "name": "노가드", "text": "서로의 기술이\n반드시 맞는다." },
  {
    "id": 100,
    "name": "시간벌기",
    "text": "상대보다 재빨라도\n행동이 느려진다."
  },
  { "id": 101, "name": "테크니션", "text": "약한 기술의\n위력이 올라간다." },
  {
    "id": 102,
    "name": "리프가드",
    "text": "맑을 때는\n상태 이상이 되지 않는다."
  },
  { "id": 103, "name": "서투름", "text": "지니고 있는 도구를\n쓸 수 없다." },
  {
    "id": 104,
    "name": "틀깨기",
    "text": "특성에 관계없이 상대에게\n기술을 쓸 수 있다."
  },
  { "id": 105, "name": "대운", "text": "상대의 급소에\n공격이 맞기 쉽다." },
  {
    "id": 106,
    "name": "유폭",
    "text": "기절할 때 스친\n상대에게 데미지를 준다."
  },
  {
    "id": 107,
    "name": "위험예지",
    "text": "상대가 지닌 위험한\n기술을 감지한다."
  },
  {
    "id": 108,
    "name": "예지몽",
    "text": "상대가 지닌 기술을\n꿰뚫어볼 수 있다."
  },
  { "id": 109, "name": "천진", "text": "상대의 능력\n변화를 무시한다." },
  { "id": 110, "name": "색안경", "text": "효과가 별로인 기술이\n강해진다." },
  {
    "id": 111,
    "name": "필터",
    "text": "효과가 굉장한 기술의\n위력을 약하게 한다."
  },
  {
    "id": 112,
    "name": "슬로스타트",
    "text": "공격과 스피드가\n잠시 동안 절반이 된다."
  },
  { "id": 113, "name": "배짱", "text": "고스트타입에\n노말 기술이 맞는다." },
  { "id": 114, "name": "마중물", "text": "물을 끌어모아\n특수공격을 올린다." },
  {
    "id": 115,
    "name": "아이스바디",
    "text": "싸라기눈일 때 HP를\n조금씩 회복한다."
  },
  {
    "id": 116,
    "name": "하드록",
    "text": "효과가 굉장한 기술의\n위력을 약하게 한다."
  },
  {
    "id": 117,
    "name": "눈퍼뜨리기",
    "text": "배틀에 나가면\n싸라기눈을 내리게 한다."
  },
  { "id": 118, "name": "꿀모으기", "text": "달콤한꿀을 모아서\n올 때가 있다." },
  { "id": 119, "name": "통찰", "text": "상대가 지닌 물건을\n알 수 있다." },
  {
    "id": 120,
    "name": "이판사판",
    "text": "반동 데미지를\n받는 기술이 강해진다."
  },
  {
    "id": 121,
    "name": "멀티타입",
    "text": "지니고 있는 플레이트에\n따라 타입이 바뀐다."
  },
  {
    "id": 122,
    "name": "플라워기프트",
    "text": "맑을 때 자신과\n같은 편이 강해진다."
  },
  {
    "id": 123,
    "name": "나이트메어",
    "text": "잠들어 있는 상대의\nHP를 줄인다."
  },
  {
    "id": 124,
    "name": "나쁜손버릇",
    "text": "닿은 상대로부터\n도구를 훔친다."
  },
  {
    "id": 125,
    "name": "우격다짐",
    "text": "힘이 강해지지만\n추가 효과가 없어진다."
  },
  { "id": 126, "name": "심술꾸러기", "text": "능력의 변화가\n역전된다." },
  {
    "id": 127,
    "name": "긴장감",
    "text": "상대를 긴장시켜 나무열매를\n먹지 못하게 한다."
  },
  { "id": 128, "name": "오기", "text": "능력이 떨어지면\n공격이 올라간다." },
  { "id": 129, "name": "무기력", "text": "HP가 절반이 되면\n능력이 떨어진다." },
  {
    "id": 130,
    "name": "저주받은바디",
    "text": "공격받으면 가끔 상대를\n사슬묶기 상태로 만든다."
  },
  {
    "id": 131,
    "name": "치유의마음",
    "text": "같은 편의 상태 이상을\n가끔 회복시킨다."
  },
  {
    "id": 132,
    "name": "프렌드가드",
    "text": "같은 편의 데미지를\n줄일 수 있다."
  },
  {
    "id": 133,
    "name": "깨어진갑옷",
    "text": "물리 기술을 받으면 방어가\n떨어지고 스피드가 올라간다."
  },
  { "id": 134, "name": "헤비메탈", "text": "자신의 무게가\n2배가 된다." },
  { "id": 135, "name": "라이트메탈", "text": "자신의 무게가\n절반이 된다." },
  {
    "id": 136,
    "name": "멀티스케일",
    "text": "HP가 꽉 찼을 때\n데미지가 줄어든다."
  },
  {
    "id": 137,
    "name": "독폭주",
    "text": "독 상태일 때 물리공격의\n위력이 올라간다."
  },
  {
    "id": 138,
    "name": "열폭주",
    "text": "화상 상태일 때 특수공격의\n위력이 올라간다."
  },
  {
    "id": 139,
    "name": "수확",
    "text": "사용한 나무열매를\n몇 번이고 만들어 낸다."
  },
  {
    "id": 140,
    "name": "텔레파시",
    "text": "같은 편의 공격의 낌새를\n읽고 기술을 받지 않는다."
  },
  { "id": 141, "name": "변덕쟁이", "text": "능력이\n오르락내리락한다." },
  { "id": 142, "name": "방진", "text": "먼지나 가루를\n막는다." },
  {
    "id": 143,
    "name": "독수",
    "text": "접촉하기만 해도 상대를\n독 상태로 만들 때가 있다."
  },
  { "id": 144, "name": "재생력", "text": "볼에 넣으면 HP가\n조금 회복된다." },
  {
    "id": 145,
    "name": "부풀린가슴",
    "text": "방어를 떨어뜨리는\n공격을 받지 않는다."
  },
  {
    "id": 146,
    "name": "모래헤치기",
    "text": "모래바람으로\n스피드가 올라간다."
  },
  {
    "id": 147,
    "name": "미라클스킨",
    "text": "변화 기술을 받기 어려운\n몸으로 되어 있다."
  },
  {
    "id": 148,
    "name": "애널라이즈",
    "text": "제일 마지막에\n기술을 쓰면 강해진다."
  },
  { "id": 149, "name": "일루전", "text": "뒤의 포켓몬으로\n둔갑하여 나온다." },
  { "id": 150, "name": "괴짜", "text": "눈앞의 포켓몬으로\n변신해버린다." },
  { "id": 151, "name": "틈새포착", "text": "상대의 벽을\n뚫고 공격한다." },
  {
    "id": 152,
    "name": "미라",
    "text": "상대의 기술을 받으면\n상대를 미라로 만든다."
  },
  {
    "id": 153,
    "name": "자기과신",
    "text": "상대를 쓰러뜨리면\n공격이 올라간다."
  },
  {
    "id": 154,
    "name": "정의의마음",
    "text": "악 기술을 받으면\n공격이 올라간다."
  },
  {
    "id": 155,
    "name": "주눅",
    "text": "주눅이 들어 스피드가\n올라가는 타입이 있다."
  },
  { "id": 156, "name": "매직미러", "text": "변화 기술을\n되받아칠 수 있다." },
  {
    "id": 157,
    "name": "초식",
    "text": "풀타입 기술을 받으면\n데미지를 입지 않고\n공격이 올라간다."
  },
  { "id": 158, "name": "짓궂은마음", "text": "변화 기술을 먼저\n쓸 수 있다." },
  {
    "id": 159,
    "name": "모래의힘",
    "text": "모래바람으로 위력이\n올라가는 기술이 있다."
  },
  { "id": 160, "name": "철가시", "text": "접촉한 상대에게\n상처를 입힌다." },
  { "id": 161, "name": "달마모드", "text": "위급할 때\n모습이 변화한다." },
  {
    "id": 162,
    "name": "승리의별",
    "text": "자신과 같은 편의\n명중률이 올라간다."
  },
  {
    "id": 163,
    "name": "터보블레이즈",
    "text": "특성에 관계없이 상대에게\n기술을 쓸 수 있다."
  },
  {
    "id": 164,
    "name": "테라볼티지",
    "text": "특성에 관계없이 상대에게\n기술을 쓸 수 있다."
  },
  {
    "id": 165,
    "name": "아로마베일",
    "text": "같은 편으로 향하는\n멘탈 공격을 막는다."
  },
  {
    "id": 166,
    "name": "플라워베일",
    "text": "같은 편의 풀타입 포켓몬은\n능력이 떨어지지 않는다."
  },
  {
    "id": 167,
    "name": "볼주머니",
    "text": "나무열매를 먹으면\nHP도 회복한다."
  },
  {
    "id": 168,
    "name": "변환자재",
    "text": "사용한 기술과 같은\n타입으로 변화한다."
  },
  { "id": 169, "name": "퍼코트", "text": "물리 기술의 데미지가\n절반이 된다." },
  {
    "id": 170,
    "name": "매지션",
    "text": "기술을 맞은 상대의\n도구를 빼앗아 버린다."
  },
  { "id": 171, "name": "방탄", "text": "구슬이나 폭탄에\n맞지 않는다." },
  {
    "id": 172,
    "name": "승기",
    "text": "능력이 떨어지면\n특수공격이 올라간다."
  },
  { "id": 173, "name": "옹골찬턱", "text": "턱이 튼튼하여\n무는 힘이 강하다." },
  {
    "id": 174,
    "name": "프리즈스킨",
    "text": "노말타입의 기술이\n얼음타입이 된다."
  },
  {
    "id": 175,
    "name": "스위트베일",
    "text": "같은 편의 포켓몬이\n잠들지 않게 된다."
  },
  { "id": 176, "name": "배틀스위치", "text": "배틀모드에서\n모습이 바뀐다." },
  {
    "id": 177,
    "name": "질풍날개",
    "text": "비행타입의 기술이\n먼저 나오게 된다."
  },
  { "id": 178, "name": "메가런처", "text": "파동 기술의\n위력이 크다." },
  { "id": 179, "name": "풀모피", "text": "그래스필드일 때\n방어가 올라간다." },
  {
    "id": 180,
    "name": "공생",
    "text": "같은 편에게 도구를\n건넬 수 있게 된다."
  },
  {
    "id": 181,
    "name": "단단한발톱",
    "text": "접촉하는 기술의\n위력이 올라간다."
  },
  {
    "id": 182,
    "name": "페어리스킨",
    "text": "노말타입의 기술이\n페어리타입이 된다."
  },
  {
    "id": 183,
    "name": "미끈미끈",
    "text": "접촉한 상대의\n스피드를 떨어뜨린다."
  },
  {
    "id": 184,
    "name": "스카이스킨",
    "text": "노말타입의 기술이\n비행타입이 된다."
  },
  { "id": 185, "name": "부자유친", "text": "부모와 자식 2마리가\n공격한다." },
  { "id": 186, "name": "다크오라", "text": "전원의 악타입\n기술이 강해진다." },
  {
    "id": 187,
    "name": "페어리오라",
    "text": "전원의 페어리타입\n기술이 강해진다."
  },
  { "id": 188, "name": "오라브레이크", "text": "오라의 효과가\n반대가 된다." },
  {
    "id": 189,
    "name": "시작의바다",
    "text": "불꽃타입의 공격을\n받지 않는 날씨로 만든다."
  },
  {
    "id": 190,
    "name": "끝의대지",
    "text": "물타입의 공격을\n받지 않는 날씨로 만든다."
  },
  {
    "id": 191,
    "name": "델타스트림",
    "text": "비행타입의 약점이\n없어지는 날씨로 만든다."
  },
  { "id": 192, "name": "지구력", "text": "공격을 받으면\n방어가 올라간다." },
  {
    "id": 193,
    "name": "도망태세",
    "text": "HP가 절반이 되면\n황급히 도망쳐서\n지닌 포켓몬으로 돌아간다."
  },
  {
    "id": 194,
    "name": "위기회피",
    "text": "HP가 절반이 되면\n위험을 회피하기 위해\n지닌 포켓몬으로 돌아간다."
  },
  {
    "id": 195,
    "name": "꾸덕꾸덕굳기",
    "text": "물타입의 기술을 받으면\n방어가 크게 올라간다."
  },
  {
    "id": 196,
    "name": "무도한행동",
    "text": "독 상태의\n상대를 공격하면\n반드시 급소에 맞는다."
  },
  {
    "id": 197,
    "name": "리밋실드",
    "text": "HP가 절반이 되면\n껍질이 깨져\n공격적으로 된다."
  },
  {
    "id": 198,
    "name": "잠복",
    "text": "교체로 나온 상대에게\n2배 데미지로 공격할 수 있다."
  },
  {
    "id": 199,
    "name": "수포",
    "text": "자신을 향하는 불꽃타입\n기술의 위력을 떨어뜨린다.\n화상을 입지 않는다."
  },
  {
    "id": 200,
    "name": "강철술사",
    "text": "강철타입 기술의\n위력이 올라간다."
  },
  {
    "id": 201,
    "name": "발끈",
    "text": "상대의 공격으로\nHP가 절반이 되면\n특수공격이 올라간다."
  },
  {
    "id": 202,
    "name": "눈치우기",
    "text": "날씨가 싸라기눈일 때\n스피드가 올라간다."
  },
  {
    "id": 203,
    "name": "원격",
    "text": "모든 기술을\n상대에게 접촉하지 않고\n사용할 수 있다."
  },
  {
    "id": 204,
    "name": "촉촉보이스",
    "text": "모든 소리 기술이\n물타입이 된다."
  },
  {
    "id": 205,
    "name": "힐링시프트",
    "text": "회복 기술을 먼저\n사용할 수 있다."
  },
  {
    "id": 206,
    "name": "일렉트릭스킨",
    "text": "노말타입 기술이\n전기타입이 된다.\n위력이 조금 올라간다."
  },
  {
    "id": 207,
    "name": "서핑테일",
    "text": "일렉트릭필드일 때\n스피드가 2배가 된다."
  },
  {
    "id": 208,
    "name": "어군",
    "text": "HP가 많을 때 무리지어 강해진다.\nHP가 얼마 남지 않으면\n무리는 뿔뿔이 흩어진다."
  },
  {
    "id": 209,
    "name": "탈",
    "text": "몸을 덮는 탈로\n1번 공격을 막을 수 있다."
  },
  {
    "id": 210,
    "name": "유대변화",
    "text": "상대를 쓰러뜨리면 트레이너와의\n유대감이 깊어져서 지우개굴닌자로\n변한다. 물수리검이 강해진다."
  },
  {
    "id": 211,
    "name": "스웜체인지",
    "text": "HP가 절반이 되면\n셀들이 응원하러 달려와\n퍼펙트폼으로 모습이 변한다."
  },
  {
    "id": 212,
    "name": "부식",
    "text": "강철타입이나 독타입도\n독 상태로 만들 수 있다."
  },
  {
    "id": 213,
    "name": "절대안깸",
    "text": "항상 비몽사몽 상태로\n절대 깨지 않는다.\n잠든 상태로 공격할 수 있다."
  },
  {
    "id": 214,
    "name": "여왕의위엄",
    "text": "상대에게 위압감을 줘서\n이쪽을 향한 선제 기술을\n사용할 수 없게 한다."
  },
  {
    "id": 215,
    "name": "내용물분출",
    "text": "상대가 쓰러뜨렸을 때\nHP의 남은 양만큼\n상대에게 데미지를 준다."
  },
  {
    "id": 216,
    "name": "무희",
    "text": "누군가 춤 기술을 쓰면\n자신도 이어서\n춤 기술을 쓸 수 있다."
  },
  {
    "id": 217,
    "name": "배터리",
    "text": "같은 편 특수 기술의\n위력을 올린다."
  },
  {
    "id": 218,
    "name": "복슬복슬",
    "text": "상대로부터 받은 접촉하는 기술의\n데미지를 반감시키지만 불꽃타입\n기술의 데미지는 2배가 된다."
  },
  {
    "id": 219,
    "name": "비비드바디",
    "text": "상대를 놀라게 해서\n이쪽을 향한 선제 기술을\n사용할 수 없게 한다."
  },
  {
    "id": 220,
    "name": "소울하트",
    "text": "포켓몬이 기절할 때마다\n특수공격이 올라간다."
  },
  {
    "id": 221,
    "name": "컬리헤어",
    "text": "공격으로 자신과 접촉한 상대의\n스피드를 떨어뜨린다."
  },
  {
    "id": 222,
    "name": "리시버",
    "text": "쓰러진 같은 편의 특성을\n이어받아 같은 특성이 된다."
  },
  {
    "id": 223,
    "name": "과학의힘",
    "text": "쓰러진 같은 편의 특성을\n이어받아 같은 특성으로 바뀐다."
  },
  {
    "id": 224,
    "name": "비스트부스트",
    "text": "상대를 기절시켰을 때\n자신의 가장 높은\n능력이 올라간다."
  },
  {
    "id": 225,
    "name": "AR시스템",
    "text": "지니고 있는 메모리로\n자신의 타입이 변한다."
  },
  {
    "id": 226,
    "name": "일렉트릭메이커",
    "text": "등장했을 때\n일렉트릭필드를 사용한다."
  },
  {
    "id": 227,
    "name": "사이코메이커",
    "text": "등장했을 때\n사이코필드를 사용한다."
  },
  {
    "id": 228,
    "name": "미스트메이커",
    "text": "등장했을 때\n미스트필드를 사용한다."
  },
  {
    "id": 229,
    "name": "그래스메이커",
    "text": "등장했을 때\n그래스필드를 사용한다."
  },
  {
    "id": 230,
    "name": "메탈프로텍트",
    "text": "상대 기술이나 특성으로\n능력을 떨어뜨릴 수 없다."
  },
  {
    "id": 231,
    "name": "스펙터가드",
    "text": "HP가 꽉 찼을 때\n받는 데미지가 줄어든다."
  },
  {
    "id": 232,
    "name": "프리즘아머",
    "text": "효과가 굉장한\n공격의 위력을\n약하게 만든다."
  },
  {
    "id": 233,
    "name": "브레인포스",
    "text": "효과가 굉장한 공격의\n위력이 더욱 올라간다."
  },
  { "id": 234, "name": "불요의검", "text": "등장했을 때\n공격이 올라간다." },
  { "id": 235, "name": "불굴의방패", "text": "등장했을 때\n방어가 올라간다." },
  {
    "id": 236,
    "name": "리베로",
    "text": "자신이 사용한 기술과\n같은 타입으로 변화한다."
  },
  {
    "id": 237,
    "name": "볼줍기",
    "text": "도구를 지니고 있지 않은 경우\n첫 번째로 던져서 실패한\n몬스터볼을 주워온다."
  },
  {
    "id": 238,
    "name": "솜털",
    "text": "공격을 받으면 솜털을 흩뿌려서\n자신을 제외한 모든 포켓몬의\n스피드를 떨어뜨린다."
  },
  {
    "id": 239,
    "name": "스크루지느러미",
    "text": "상대의 기술을 끌어모으는\n특성이나 기술의\n영향을 받지 않는다."
  },
  {
    "id": 240,
    "name": "미러아머",
    "text": "자신이 받는\n능력 다운 효과에 한해\n되받아친다."
  },
  {
    "id": 241,
    "name": "그대로꿀꺽미사일",
    "text": "파도타기나 다이빙을 쓰면\n먹이를 물어온다. 데미지를\n받으면 먹이를 토해내서 공격한다."
  },
  {
    "id": 242,
    "name": "굳건한신념",
    "text": "상대의 기술을 끌어모으는\n특성이나 기술의\n영향을 받지 않는다."
  },
  {
    "id": 243,
    "name": "증기기관",
    "text": "물타입이나 불꽃타입\n기술을 받으면\n스피드가 매우 크게 올라간다."
  },
  {
    "id": 244,
    "name": "펑크록",
    "text": "소리 기술의 위력이 올라간다.\n상대로부터 받는 소리 기술의\n데미지는 절반이 된다."
  },
  {
    "id": 245,
    "name": "모래뿜기",
    "text": "공격을 받으면\n모래바람을 일으킨다."
  },
  {
    "id": 246,
    "name": "얼음인분",
    "text": "얼음인분의 보호를 받아\n특수공격으로 받는\n데미지가 절반이 된다."
  },
  {
    "id": 247,
    "name": "숙성",
    "text": "나무열매를\n숙성시켜서\n효과가 2배가 된다."
  },
  {
    "id": 248,
    "name": "아이스페이스",
    "text": "물리공격을 머리의 얼음이\n대신 맞아주지만 모습도 바뀐다.\n얼음은 싸라기눈이 내리면 원래대로 돌아온다."
  },
  {
    "id": 249,
    "name": "파워스폿",
    "text": "옆에 있기만 해도\n기술의 위력이 올라간다."
  },
  {
    "id": 250,
    "name": "의태",
    "text": "필드의 상태에 따라\n포켓몬의 타입이 바뀐다."
  },
  {
    "id": 251,
    "name": "배리어프리",
    "text": "등장했을 때 상대와 같은 편의\n빛의장막, 리플렉터, 오로라베일의\n효과가 사라진다."
  },
  {
    "id": 252,
    "name": "강철정신",
    "text": "같은 편의 강철타입\n공격의 위력이 올라간다."
  },
  {
    "id": 253,
    "name": "멸망의바디",
    "text": "접촉하는 기술을 받으면\n3턴 후에 양쪽 모두 기절한다.\n교체되면 효과가 없어진다."
  },
  {
    "id": 254,
    "name": "떠도는영혼",
    "text": "접촉하는 기술로\n공격해온 포켓몬과\n특성을 바꾼다."
  },
  {
    "id": 255,
    "name": "무아지경",
    "text": "공격이 올라가지만\n처음에 선택한\n기술 외에는 쓸 수 없게 된다."
  },
  {
    "id": 256,
    "name": "화학변화가스",
    "text": "화학변화가스를 가진 포켓몬이 배틀에 나와 있으면\n모든 포켓몬이 가진 특성의 효과가\n사라지거나 발동하지 않게 된다."
  },
  {
    "id": 257,
    "name": "파스텔베일",
    "text": "자신과 같은 편이\n독의 상태 이상 효과를\n받지 않게 된다."
  },
  {
    "id": 258,
    "name": "꼬르륵스위치",
    "text": "턴이 끝날 때마다 배부른 모양,\n배고픈 모양, 배부른 모양...으로\n번갈아서 모습을 바꾼다."
  },
  { "id": 259, "name": "퀵드로", "text": "상대보다 먼저\n행동할 수도 있다." },
  {
    "id": 260,
    "name": "보이지않는주먹",
    "text": "상대에게 접촉하는 기술을 사용하면\n방어의 효과를 무시하고 공격할 수 있다."
  },
  {
    "id": 261,
    "name": "기묘한약",
    "text": "등장했을 때\n조개껍질에서 약을 흩뿌려서\n능력 변화를 원래대로 되돌린다."
  },
  {
    "id": 262,
    "name": "트랜지스터",
    "text": "전기타입 기술의\n위력이 올라간다."
  },
  {
    "id": 263,
    "name": "용의턱",
    "text": "드래곤타입 기술의\n위력이 올라간다."
  },
  {
    "id": 264,
    "name": "백의울음",
    "text": "상대를 쓰러뜨리면\n차가운 울음소리를 내면서\n공격이 올라간다."
  },
  {
    "id": 265,
    "name": "흑의울음",
    "text": "상대를 쓰러뜨리면\n무서운 울음소리를 내면서\n특수공격이 올라간다."
  },
  {
    "id": 266,
    "name": "혼연일체",
    "text": "버드렉스의 긴장감과\n블리자포스의 백의울음\n두 가지 특성을 겸비한다."
  }
]
